        },
        "position": {
          "description": "Position at the company",
          "$ref": "#/definitions/Position"
        },
        "birthYear": {
          "description": "Person's birth year",
          "$ref": "#/definitions/BirthYear"
        }
      },
      "required": [
        "name",
        "age",
        "position",
        "birthYear"
      ]
    },
    "Status": {
      "name": "Status",
      "properties": {
        "log": {
          "type": "boolean",
          "description": "is log ????"
        },
        "member": {
          "type": "boolean",
          "description": "Is a prime member ?"
        }
      }
    },
    "Position": {
      "name": "position",
      "properties": {
        "lat": {
          "type": "number",
          "format": "double",
          "description": "Latitude"
        },
        "lng": {
          "type": "number",
          "format": "double",
          "description": "Longitude"
        }
      }
    },
    "BirthYear": {
      "name": "birthyear",
      "properties": {
        "month": {
          "type": "integer",
          "description": "Birth month"
        },
        "year": {
          "type": "integer",
          "description": "Birth year"
        }
      }
    }
  }
