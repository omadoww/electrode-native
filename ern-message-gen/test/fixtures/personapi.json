            "description": "Person object",
            "schema": {
              "$ref": "#/definitions/Person"
            }
          }
        }
      }
    }
  },
  "definitions": {
    "Person": {
      "properties": {
        "name": {
          "type": "string",
          "description": "Person's name"
        },
        "age": {
          "type": "integer",
          "description": "Persons Age"
        },
        "hiredMonth": {
          "type": "integer",
          "description": "Month hired"
        },
        "status": {
          "description": "Current employment status",
