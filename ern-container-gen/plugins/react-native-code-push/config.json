{
  "android": {
    "moduleName": "app"
  },
  "ios": {
    "copy": [
      { "source": "ios/**", "dest": "ElectrodeContainer/Libraries/CodePush" }
    ],
    "pbxproj": {
      "addStaticLibrary": [
        "libz.tbd"
      ],
       "addProject": [
        { "path": "CodePush/CodePush.xcodeproj", "group": "Libraries", "staticLibs": [ { "name": "libCodePush.a", "target": "CodePush" } ] }
      ],
      "addHeaderSearchPath": [
        "\"$(SRCROOT)/ElectrodeContainer/Libraries/CodePush/**\""
      ]
    }
  }
}
