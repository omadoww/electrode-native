{
  "android": {
    "moduleName": "electrode-reactnative-bridge"
  },
  "ios": {
    "copy": [
      { "source": "ios/**", "dest": "ElectrodeContainer/Libraries/ElectrodeReactNativeBridge" }
    ],
    "pbxproj": {
      "addFile": [
        { "path": "ElectrodeReactNativeBridge/ElectrodeReactNativeBridge.xcodeproj", "group": "Libraries" }
      ],
      "addFramework": [
        "ElectrodeReactNativeBridge.framework"
      ],
      "addHeaderSearchPath": [
        "\"$(SRCROOT)/ElectrodeContainer/Libraries/ElectrodeReactNativeBridge/**\""
      ]
    },
    "groupName": "ElectrodeReactNativeBridge"
  }
}
